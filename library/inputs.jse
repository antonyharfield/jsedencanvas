${{
	$('body').delegate("#canvas", "click", function(e) {
		var canvas = $('#canvas');
		root.lookup('mouseClickX').assign(e.pageX-parseInt(canvas.css("left")));
		root.lookup('mouseClickY').assign(e.pageY-parseInt(canvas.css("top")));
	});
}}$;
